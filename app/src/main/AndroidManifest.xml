<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.bhram.radhakrishnavachanpk">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity android:name="com.bhram.radhakrishnavachanpk.PhotoActivity"></activity>
        <activity
            android:name="com.bhram.radhakrishnavachanpk.UserId"
            android:theme="@style/FullScreen" />
        <activity
            android:name="com.bhram.radhakrishnavachanpk.MainActivity"
            android:theme="@style/FullScreen" />
        <activity
            android:name="com.bhram.radhakrishnavachanpk.SplashScreen"
            android:theme="@style/FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-2979694091012217~2461819022"/>
    </application>

</manifest>